{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "backend/src/index.ts",
    "frontend/app/**/*.tsx",
    "frontend/app/**/*.ts"
  ],
  "project": [
    "backend/src/auth/**/*.{ts,tsx}",
    "backend/src/middleware/auth*.ts",
    "backend/src/controllers/auth*.ts",
    "backend/src/services/auth*.ts",
    "backend/src/repositories/user*.ts",
    "frontend/components/auth/**/*.{ts,tsx}",
    "frontend/contexts/auth*.{ts,tsx}",
    "frontend/hooks/use-auth*.{ts,tsx}",
    "frontend/providers/auth*.{ts,tsx}"
  ],
  "ignore": ["**/*.d.ts", "**/*.test.*", "**/*.spec.*"],
  "ignoreDependencies": ["@sentry/nextjs"],
  "ignoreExportsUsedInFile": true
}